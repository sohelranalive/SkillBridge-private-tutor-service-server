model Booking {
    booking_id   String   @id @default(uuid())
    booking_slot DateTime
    start_time   DateTime
    end_time     DateTime
    student_id   String
    tutor_id     String

    student Users @relation(fields: [student_id], references: [user_id])

    tutor TutorProfile @relation(fields: [tutor_id], references: [tutor_id])
}
